<%= form_with model: [:admin, @user] do |f| %>
  <%= f.text_field :name, required: true %>
  <%= f.text_field :email %>
  <%= f.file_field :avatar, direct_upload: true %>
  <%= render 'active_storage_ext/attachments/list_field', target: @user.avatar %>
  <%= f.text_field :mobile %>
  <%= f.text_field :password %>
  <%= f.check_box :disabled %>
  <%= f.submit %>
<% end %>
