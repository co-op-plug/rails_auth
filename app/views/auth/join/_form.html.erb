<%= f.hidden_field :identity, value: params[:identity] %>
<%= f.text_field :token, placeholder: VerifyToken.human_attribute_name(:token), autocomplete: false %>
<%= f.password_field :password, placeholder: User.human_attribute_name(:password), autocomplete: false %>
<%= f.password_field :password_confirmation, placeholder: User.human_attribute_name(:password_confirmation), autocomplete: false %>
<%= f.submit %>
